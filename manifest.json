{
  "manifest_version": 2,

  "name": "Seahorse",
  "description": "Discrimination-free browsing",
  "version": "0.1",

  "permissions": [
    "tabs",
    "storage",
    "webRequest"
  ],

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"],
      "run_at": "document_idle",
      "all_frames" : false
    }
  ]
}
