{
  "manifest_version": 2,

  "name": "Seahorse",
  "description": "Discrimination-free browsing",
  "version": "0.1",

  "permissions": [
    "tabs"
  ],

  "content_scripts": [
    {
      "matches": [
        "*://www.linkedin.com/*",
        "*://www.devpost.com/*",
        "*://www.github.com/*"
      ],
      "js": ["jquery-3.4.1.min.js", "content.js"]
    }
  ]
}
