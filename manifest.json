{
  "manifest_version": 2,

  "name": "Seahorse",
  "description": "Discrimination-free browsing",
  "version": "1.0",

  "permissions": ["tabs", "storage"],

  "content_scripts": [
    {
      "matches": [
        "*://www.linkedin.com/*",
        "*://www.devpost.com/*",
        "*://www.github.com/*"
      ],
      "js": ["jquery-3.4.1.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
